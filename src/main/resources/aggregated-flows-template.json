{
  "index_patterns": ["aggregated-flows-*"],
  "settings": {
    "number_of_shards": 1
  },
  "mappings": {
    "_source": {
      "enabled": false
    },
    "properties": {
      "@timestamp": {
        "type": "date",
        "format": "epoch_millis"
      },
      "range_start": {
        "type": "date",
        "format": "epoch_millis"
      },
      "range_end": {
        "type": "date",
        "format": "epoch_millis"
      },

      "grouped_by": {
        "dynamic": true,
        "type": "object",
        "properties": {
          "application": {
            "type": "boolean"
          }
        }
      },

      "ranking": {
        "type": "integer"
      },
      "bytes_ingress": {
        "type": "long"
      },
      "bytes_egress": {
        "type": "long"
      },

      "exporter": {
        "dynamic": true,
        "type": "object",
        "properties": {
          "foreign_source": {
            "type": "keyword",
            "norms": false
          },
          "foreign_id": {
            "type": "keyword",
            "norms": false
          },
          "node_id": {
            "type": "integer"
          }
        }
      },
      "ifindex": {
        "type": "integer"
      },
      "application": {
        "type": "keyword",
        "norms": false
      },
      "convo_key": {
        "type": "keyword",
        "norms": false
      }
    }
  }
}