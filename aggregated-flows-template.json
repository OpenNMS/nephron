{
  "index_patterns": ["aggregated-flows-*"],
  "settings": {
    "number_of_shards": 1
  },
  "mappings": {
    "_source": {
      "enabled": false
    },
    "properties": {
      "@timestamp": {
        "type": "date",
        "format": "epoch_millis"
      },
      "context": {
        "type": "keyword",
        "norms": false
      }
    }
  }
}